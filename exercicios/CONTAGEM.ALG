algoritmo "CONTAGEM"
var
   cont, n, m: inteiro
inicio
      escreva("A partir de que número você quer contar? ")
      leia(n)
      cont <- n
      escreva("Até que número você quer contar? ")
      leia(m)
      enquanto (cont <= m) faca
               escreval(cont)
               cont <- cont + 1
      fimenquanto
fimalgoritmo